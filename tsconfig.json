{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "types/*": ["src/client/types/*"]
        },
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "lib": [
            "dom",
            "es2018"
        ],
        "module": "ES2015",
        "target": "es5",
        "jsx": "react",
    }
}